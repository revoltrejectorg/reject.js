{
    "type": "module",
    "name": "reject.js",
    "version": "1.0.1",
    "description": "Converts Discord.js API calls into Revolt.js API calls",
    "author": "V3L0C1T13S",
    "main": "dist/index.js",
    "files": [
        "dist/**/*"
    ],
    "license": "MIT",
    "keywords": [
        "revolt.js",
        "revolt.chat",
        "revolt",
        "compatibility"
    ],
    "scripts": {
        "build": "tsc",
        "watch": "tsc -w",
        "clean": "rimraf dist",
        "prepare": "npm run clean && npm run build",
        "version": "git add -A src",
        "postversion": "git push && git push --tags"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/V3L0C1T13S/reject.js.git"
    },
    "dependencies": {
        "discord.js": "^13.7.0",
        "revolt.js": "^6.0.0-2"
    },
    "devDependencies": {
        "@types/node": "^17.0.34",
        "rimraf": "^3.0.2",
        "typescript": "^4.6.4"
    }
}
