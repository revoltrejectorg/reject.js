{
    "name": "revolt-reject.js",
    "version": "3.0.0",
    "description": "Converts Discord.js API calls into Revolt.js API calls",
    "author": "V3L0C1T13S",
    "main": "dist/index.js",
    "module": "esm/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "esm"
    ],
    "license": "MIT",
    "keywords": [
        "revolt.js",
        "revolt.chat",
        "revolt",
        "compatibility"
    ],
    "scripts": {
        "build": "tsc && tsc -p tsconfig.cjs.json",
        "dev": "tsc -w",
        "typecheck": "tsc --noEmit",
        "lint": "eslint src/**/*.ts",
        "clean": "rimraf dist esm tsconfig.tsbuildinfo",
        "prepare": "npm run clean && npm run build",
        "version": "git add -A src",
        "postversion": "git push && git push --tags"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/revoltrejectorg/reject.js.git"
    },
    "dependencies": {
        "axios": "^0.27.2",
        "discord-api-types": "^0.37.8",
        "discord.js": "^13.8.0",
        "form-data": "^4.0.0",
        "revolt.js": "^6.0.12"
    },
    "devDependencies": {
        "@types/node": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "eslint": "^8.16.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.26.0",
        "rimraf": "^3.0.2",
        "typescript": "^4.7.2"
    }
}
